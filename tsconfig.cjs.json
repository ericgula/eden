{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es2015", "dom"],
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "jsx": "react",
    "noImplicitAny": false,
    "importHelpers": false,
    "downlevelIteration": true
  },
  "exclude": ["node_modules", "dist", "pages/**/*", "next-env.d.ts"]
}
